configuration {
  drun-display-format: "{name}";
  location: 6;
  font: "CodeNewRoman Nerd Font Regular 12";

  /* Keybindings */   
  kb-move-word-back: "";
  kb-remove-to-eol: "";
  kb-accept-entry: "Return";
  kb-row-up: "Up,Control+k";
  kb-row-down: "Down,Control+j";
}

/* Pywal theme. */
@theme "~/.cache/wal/colors-rofi-custom.rasi"

/* If you want to import default pywal theme then
@import "~/.cache/wal/colors-rofi-dark.rasi"
*/
