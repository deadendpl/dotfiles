{
  "positionX": "right",
  "positionY": "top",
  "layer": "overlay",
  "cssPriority": "user",
  "notification-icon-size": 80,
  "notification-body-image-height": 100,
  "notification-body-image-width": 200,
  "timeout": 5,
  "timeout-low": 3,
  "control-center-height": 600,
  /* default 500 */
  "notification-window-width": 470,
  "image-visibility": "when-available",
  // "transition-time": 200,
  "hide-on-clear": true,
  "hide-on-action": true,
  "script-fail-notify": true,
  "widgets": [
    "notifications",
    "mpris"
  ],
  "widget-config": {
    "mpris": {
      "image-size": 96,
      "image-radius": 13
    }
  }
}
